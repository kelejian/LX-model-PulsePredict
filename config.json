{
    "name": "PulseCNN_GauNLL",
    "n_gpu": 1,
    "arch": {
        "type": "PulseCNN",
        "args": {
            "GauNll_use": true,
            "mlp_latent_dim": 256,
            "mlp_num_layers": 2,
            "projection_init_channels": 32,
            "UpLayer_ks": 3,
            "channels_list": [256, 128, 64],
            "output_lengths": [38, 75, 150]
        }
    },
    "data_loader_train": {
        "type": "CollisionDataLoader",
        "args": {
            "packaged_data_path": "E:/WPS Office/1628575652/WPS企业云盘/清华大学/我的企业文档/课题组相关/理想项目/仿真数据库相关/acc_data_before0915/packaged_data_train.npz",
            "batch_size": 512,
            "shuffle": true,
            "validation_split": 0,
            "num_workers": 0,
            "pulse_norm_mode": "absmax",
            "scaler_path": "data_loader/pulse_scaler.joblib",
            "training": true
        }
    },
    "data_loader_test": {
        "type": "CollisionDataLoader",
        "args": {
            "packaged_data_path": "E:/WPS Office/1628575652/WPS企业云盘/清华大学/我的企业文档/课题组相关/理想项目/仿真数据库相关/acc_data_before0915/packaged_data_test.npz",
            "batch_size": 32,
            "shuffle": false,
            "validation_split": 0,
            "num_workers": 0,
            "pulse_norm_mode": "absmax",
            "scaler_path": "data_loader/pulse_scaler.joblib",
            "training": false
        }
    },
    "optimizer": {
        "type": "AdamW",
        "args": {
            "lr": 0.0003,
            "weight_decay": 0.0012
        }
    },
    "loss": [
        {
            "type": "MultiLoss",
            "weight": 1.0,
            "args": {
                "loss_weights": [1.0, 2.0, 5.0],
                "base_loss_type": "GaussianNLLLoss",
                "base_loss_args": {
                    "eps": 1e-06,
                    "reduction": "mean"
                }
            }
        }
    ],
    "metrics": [
        "rmse_x",
        "rmse_y",
        "rmse_z",
        "iso_rating_x",
        "iso_rating_y",
        "iso_rating_z"
    ],
    "lr_scheduler": {
        "type": "CosineAnnealingLR",
        "args": {
            "T_max": 60,
            "eta_min": 2e-7
        }
    },
    "trainer": {
        "epochs": 60,
        "save_dir": "saved/",
        "save_period": 10,
        "verbosity": 2,
        "monitor": "max iso_rating_x",
        "early_stop": 100,
        "tensorboard": true
    }
}